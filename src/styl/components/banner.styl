.fullbanner
    margin-top: 148px;
    .slick-dots
        bottom: 30px
    @media(max-width: 991px)
        margin-top: 160px;
        .fbits-componente-banner .fbits-banner-item 
            a 
                img
                    width 230% !important
                    max-width 230%
                    left 50%
                    transform: translateX(-50%)

.fullbanner
    .slick-prev
        left 0
        @media(max-width : 991px)
            left 5px !important
            width: 30px
            height 30px
    .slick-next
        right 0
        @media(max-width : 991px)
            right 5px !important
            width: 30px
            height 30px
    img
        max-width 100%
        margin: 0 auto
        @media(max-width : 991px)
            display: block
            position relative
            width: auto
            max-width: 149%
            left 50%
            transform: translateX(-50%)
    .fbits-banner-item
        overflow hidden


.mini-banner
    &__container
        max-width: 1330px
        padding: 0 0.94rem
        width: 100%
        margin: 0 auto
    &__wrapper
        display flex
        align-items: center
        justify-content: space-between
        flex-wrap: wrap
        padding-top: 50px
        padding-bottom: 55px
        @media(max-width : 767px)
            padding-top: 20px
        .fbits-banner-item
            flex: 0 1 33%
            overflow hidden
            display: flex
            justify-content: center
            
            @media(max-width : 767px)
                flex: 0 1 100%
                margin-bottom: 15px
            img
                transition: all .3s
                display: block
                max-width: 100%
                @media(max-width : 767px)
                    margin: 0 auto
                &:hover
                    transform: scale(1.1)

.mini-footer
    &__container
        max-width: 1330px
        padding: 0 0.94rem
        width: 100%
        margin: 0 auto
    &__wrapper
        padding-bottom: 40px
        padding-top: 60px
        @media(max-width : 767px)
            padding-bottom: 20px
            padding-top: 0px
    &__title
        font-size: 35px
        color: #434343
        line-height: 32px
        font-weight: 900
        text-transform: uppercase
        text-align: center
        padding-bottom: 20px
        @media(max-width : 767px)
            font-size: 20px
            padding-bottom: 25px
    &__banners
        display flex
        align-items: center
        justify-content: space-between
        flex-wrap: wrap
        @media(max-width : 767px)
            justify-content: center
        .fbits-banner-item
            flex: 0 1 48%
            overflow hidden
            @media(max-width : 767px)
                flex: 0 1 100%
                margin-bottom: 25px
        img 
            display: block
            transition: all .3s
            max-width 100%
            @media(max-width : 991px)
                margin: 0 auto
            &:hover
                transform: scale(1.1)
    .fbits-banner-item
        overflow hidden


.full__banner-beneficios 
    
    display: flex
    align-items: center
    justify-content: center
    ul
        display: flex
        align-items: center
        justify-content: space-between
        flex-wrap: wrap
        max-width 1330px
        padding 0 15px
        min-height 70px
        width: 100%

        li
            display: flex
            align-items: center
            position relative

            @media(max-width 991px)
                display: flex!important
                justify-content center

                img
                    margin 0 auto
                    left 0
                    transform none

            &:after
                content: ''
                width 1px
                background #ccc
                height 40px
                position: absolute
                right: -60px
                opacity: .3

                @media(max-width 991px)
                    display none

            &:last-child
                &:after
                    display none

.full__brands
    display: flex
    align-items: center
    justify-content: space-between
    flex-wrap: wrap
    max-width 1330px
    padding 0 15px
    margin 50px auto

    @media(max-width 991px)
        display none

    span
        font-size 25px
        color: #1c1d1d
        border-left: 5px solid #ee3a53
        font-weight: 700
        padding-left: 15px

    .fbits-listaMarcas-item
        padding 30px 50px

.middle-banner
    display: flex
    justify-content: space-between
    align-items: center

.fbits-componente-banner
    .fbits-banner-item
        a
            width: 100% !important
            display block
            img 
                margin 0 auto !important
                width: 100% !important

.fbits-banner-rodape
    max-width: 100% !important
    padding 40px 0 !important
    background-image: url('data:image/png;base64,') !important
    a
        img 
            max-width: 555px !important

.fbits-banner-rodape
    margin-bottom 0 !important
    padding-bottom 100px !important
    .antes-depois
        max-width: 1190px
        margin: 0 auto
        .line
            margin-bottom 40px
        &__title
            &.big
                background-color transparent
                font-size: 40px
                font-weight: 300
                background-color transparent
                color: #3c3c3c
                font-family: 'Andora Demo';
                font-style: normal;
                text-transform: initial;
                span 
                    color: $cor01
        &-text
            font-size: 18px
            font-weight: 600
            text-align center
            margin 0 auto 
            display block
            color: #3c3c3c
            @media(max-width: 355px)
                font-size: 12px
        .slick-dots
            bottom: -50px !important
        @media(max-width: 1290px)
            max-width: 991px
            .fbits-banner-item
                margin: 0 10px


section.full__banner
    display: flex
    align-items: center
    justify-content: center
    max-width: 1580px
    margin: 94px auto
    padding: 0 15px

    .full__banner-right
        max-width 582px
        .fbits-banner-item 

            a, img
                display: block
                max-height: 268px

.show__product
    max-width: 1580px
    width: 100%
    margin: 0 auto
    padding: 0 0.94rem

    > h2
        font-size: 24px
        font-family: 'Bison-Bold'
        color: #000
        margin-bottom 10px

    .spot
        width 100%
        background: #e5e5e5 

        .spotContent a
            float: none 

            img
                margin: 0 auto

    .name__product
        font-family: 'Bison-Bold'
        color: #000
        font-size: 16px
        background: #fff
        width: 50%
        position: absolute
        left: 15px
        top: 15px
        text-align: left
        padding-left: 5px
        line-height: 18px

    .precoPor
        font-family: 'Bison-Bold'
        color: #000
        font-size: 16px
        text-align: left

    .bottom__show
        position: absolute
        bottom: 0
        display: flex
        align-items: center
        width 100%
        align-items: center

        .spotPreco
            margin: 0!important
            height auto!important
            min-height: 0
            padding: 0

        .bt.adicionar
            display: none

        .botoesSpot
            display: block
            text-align: right

        .bt.comprar
            background-image: url('data:image/png;base64,')!important
            background-color: transparent
            font-size: 0
            width 40px!important
            background-size: 100%
            background-position: center
